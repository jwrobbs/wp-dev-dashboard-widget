    "autoload": {
        "psr-4": {
            "WPDevDashboardWidget\\": "src/",
        "WPDevDashboardWidget\\Modules\\SiteInfo\\": "modules/site_info/src/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*"
    },
    "authors": [
        {
            "name": "Cascade AI (on behalf of jwrobbs)",
            "email": "noreply@example.com"
        }
    ]
}
